{
	"version": 3,
	"routes": [
	  {
		"src": "^/_astro/(.*)$",
		"headers": {
		  "cache-control": "public, max-age=31536000, immutable"
		},
		"continue": true
	  },
	  {
		"handle": "filesystem"
	  },
	  {
		"src": "^/blog/([^/]+?)/?$",
		"dest": "/blog/$1/index.html"
	  },
	  {
		"src": "^/blog/?$",
		"dest": "/blog/index.html"
	  },
	  {
		"src": "^/projects/?$",
		"dest": "/projects/index.html"
	  },
	  {
		"src": "^/rss.xml/?$",
		"dest": "/rss.xml"
	  },
	  {
		"src": "^/$",
		"dest": "/index.html"
	  }
	]
  }
  